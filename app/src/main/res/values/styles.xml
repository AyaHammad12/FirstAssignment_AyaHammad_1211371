<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="textStyle">
        <item name="android:textColor" >#FFFFFF</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>

        <!--0F1432-->
    </style>
    <style name="textStyleTop">
        <item name="android:textColor" >#FFFFFF</item>
        <item name="android:layout_marginTop">30dp</item>
        <item name="android:layout_marginBottom">20dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16dp</item>
<!--        android:layout_marginTop="30dp"-->
<!--        android:layout_marginBottom="20dp"-->

    </style>
    <style name="EditTextStyle" parent="Widget.AppCompat.EditText">
        <item name="android:textColor">#FAF3F3</item>
        <item name="android:textSize">13sp</item>
        <item name="android:layout_gravity">right</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColorHint">#EFEBEB</item>
    </style>

    <style name="stylle">
        <item name="android:layout_width">200dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_marginBottom">34dp</item>
        <item name="android:fontFamily">sans-serif-black</item>
    </style>
    <style name="tableStyleIN">
        <item name="android:ems">10</item>
        <item name="android:inputType">numberDecimal</item>
        <item name="android:textColorHint" >#FFFFFF</item>
        <item name="android:textColor" >#FFFFFF</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="textStyle2">
        <item name="android:textColor" >#FFFFFF</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:layout_gravity">right</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>

    </style>
    <style name="textStyle21">
        <item name="android:textColor" >#FFFFFF</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:layout_gravity">right</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>

    </style>

    <style name="theam">
       <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_margin">13dp</item>

    </style>
</resources>